<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="simply_stream_souls_death.event_subscriber.tracker_command_event_subscriber"
                 class="SimplyStream\SoulsDeathBundle\EventSubscriber\TrackerCommandEventSubscriber">
            <argument key="$userRepository" type="service" id="soulsdeath.repository.user"/>
            <argument key="$trackerRepository" type="service" id="soulsdeath.repository.tracker"/>
            <argument key="$counterRepository" type="service" id="soulsdeath.repository.counter"/>
            <argument key="$entityManager" type="service" id="doctrine.orm.entity_manager.default"/>
            <argument key="$hub" type="service" id="Symfony\Component\Mercure\HubInterface"/>
            <argument key="$producer" type="service" id="old_sound_rabbit_mq.send_chat_message_producer"/>

            <tag name="kernel.event_subscriber"/>
        </service>
    </services>
</container>

